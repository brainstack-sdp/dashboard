doctype html
html
    head
        title HP Education Analysis
        link(rel="stylesheet" href="styles/vendor-aa67210ead.css")
        link(rel="stylesheet" href="styles/app-fe96e7be2d.css")
        link(rel="stylesheet" href="styles/style.css")
    body.blur-theme
        header
            .page-top.clearfix(scroll-position="scrolled" max-height="50")
                .col-xs-4
                    a(href="/" class="al-logo clearfix")
                        img(src="/assets/img/himachal-pradesh-sarkar.png")
                        span HP
                        | Assessement Data
                .col-xs-4.text-center
                    img(src="/assets/img/samarth-logo-v9---lowres.png")
                .col-xs-4.text-right
                    img(src="/assets/img/ssabglogo.jpg")
        .body-bg
        main
            aside.al-sidebar
                ul.al-sidebar-list
                    li
                        h4 District
                        select(class="form-control js-filter" data-type="district")
                    li
                        h4 Block
                        select(class="form-control js-filter" data-type="block")
                    li
                        h4 Cluster
                        select(class="form-control js-filter" data-type="cluster")
                    li
                        h4 School
                        select(class="form-control js-filter" data-type="school_name")
                    li
                        h4 Summer/Winter
                        select(class="form-control js-filter" data-type="summer_winter")
                            option(value="summer") Summer
                            option(value="winter") Winter
                    li
                        h4 Class
                        select(class="form-control js-filter" data-type="class_code")
                            option(value="") All
                            option(value="1") Class 1
                            option(value="2") Class 2
                            option(value="3") Class 3
                            option(value="4") Class 4
                            option(value="6") Class 6
                            option(value="7") Class 7
                    li
                        h4 Subject
                        select(class="form-control js-filter" data-type="subject")
                            option(value="") All
                            option(value="1") Hindi
                            option(value="2") Maths
                            option(value="3") EVS
                            option(value="4") English
                            option(value="5") SST
                            option(value="6") Science
                    li
                        h4 Gender
                        select(class="form-control js-filter" data-type="sex")
                            option(value="") All
                            option(value="1") Male
                            option(value="2") Female
                    li
                        h4 Caste
                        select(class="form-control js-filter" data-type="category")
                            option(value="") All
                            option(value="0") SC
                            option(value="1") ST
                            option(value="2") OBC
                            option(value="3") Muslim
                            option(value="4") General
                    li
                        h4 Competency
                        select(class="form-control js-filter" data-type="competency")

            .chart-main.al-main
                .al-content
                    .pull-right
                        a.btn(href="/") Reset Filters
                    section.chartist
                    .col-md-12
                        .panel.panel-blur
                            .panel-heading
                                h3.panel-title 1. Overall Grade Distribution
                            .panel-body
                                .ct-chart(id="gradePie" style="width: 100%; height: 400px;")
                                .text-right No. of students assessed(approx.):&nbsp;
                                    span.js-access
                                .text-right No. of students enrolled:&nbsp;
                                    span.js-enroll -
                                .preloader.js-loader.js-gradePie
                                    div
                    .col-md-12
                        .panel.panel-blur
                            .panel-heading
                                h3.panel-title 2. Subject Learning Achievement
                            .panel-body
                                div(id='subjectStack' style="width: 100%; height: 400px;")
                                .text-right No. of students assessed(approx.):&nbsp;
                                    span.js-access
                                .text-right No. of students enrolled:&nbsp;
                                    span.js-enroll -
                                .preloader.js-loader.js-subjectStack
                                    div
                    .col-md-12
                        .panel.panel-blur
                            .panel-heading
                                h3.panel-title 3. Class Learning Achievement
                            .panel-body
                                div(id="classStack" style="width: 100%; height: 400px;")
                                .text-right No. of students assessed(approx.):&nbsp;
                                    .js-access
                                .text-right No. of students enrolled:&nbsp;
                                    span.js-enroll -

                                .preloader.js-loader.js-classStack
                                    div

                    .col-md-12
                        .panel.panel-blur
                            .panel-heading
                                h3.panel-title 4. Districtwise Grade Distribution
                            .panel-body
                                .ct-chart(id="gradeStack" style="width: 100%; height: 400px;")
                                .text-right No. of students assessed(approx.):&nbsp;
                                    span.js-access
                                .text-right No. of students enrolled:&nbsp;
                                    span.js-enroll
                                .preloader.js-loader.js-gradeStack
                                    div
                    .col-md-12
                        .panel.panel-blur
                            .panel-heading
                                h3.panel-title 5. Overall Competency Trends
                            .panel-body
                                .ct-chart(id="competencyTrends" style="width: 100%; height: 400px;")
                                .text-right No. of students assessed(approx.):&nbsp;
                                    span.js-access
                                .text-right No. of students enrolled:&nbsp;
                                    span.js-enroll
                                .preloader.js-loader.js-competencyTrends
                                    div
                    .col-md-12
                        .panel.panel-blur
                            .panel-heading
                                h3.panel-title 6. Competency Category Analysis
                            .panel-body
                                .ct-chart(id="competencyCategory" style="width: 100%; height: 400px;")
                                .text-right No. of students assessed(approx.):&nbsp;
                                    span.js-access
                                .text-right No. of students enrolled:&nbsp;
                                    span.js-enroll
                                .preloader.js-loader.js-competencyCategory
                                    div
                    .col-md-12
                        .panel.panel-blur
                            .panel-heading
                                h3.panel-title 7. Competency Achievement
                            .panel-body
                                .ct-chart(id="competency" style="width: 100%; height: 400px;")
                                .text-right No. of students assessed(approx.):&nbsp;
                                    span.js-access
                                .text-right No. of students enrolled:&nbsp;
                                    span.js-enroll -
                                .preloader.js-loader.js-competency
                                    div
                    .col-md-12
                        .panel.panel-blur
                            .panel-heading
                                h3.panel-title 8. Competencywise Achievement
                            .panel-body
                                .ct-chart(id="competencyAcheivement" style="width: 100%; height: 400px;")
                                .text-right No. of students assessed(approx.):&nbsp;
                                    span.js-access
                                .text-right No. of students enrolled:&nbsp;
                                    span.js-enroll -
                                .preloader.js-loader.js-competencyAcheivement
                                    div
            footer.al-footer.clearfix
                .al-footer-right Created by Samarth
            .modal.fade.js-modal
                .modal-dialog.modal-lg
                    .modal-content
                        .modal-header
                            button(type="button" class="close js-close"  aria-label="Close")
                                em(class="ion-ios-close-empty sn-link-close")
                            h4.modal-title Competency Category Drilldown
                        .modal-body
                            div(id="catDrill" style="width: 640px; height: 300px;")
                            .preloader.js-loader.js-catDrill
                                div
        //div( id="preloader")
            div
        script(src="js/vendor-727ff9d081.js")
        script(src="js/app.js")
